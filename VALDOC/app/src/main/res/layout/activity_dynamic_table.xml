<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.project.valdoc.RDACPHAVUserEntryActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="575dp">
<ScrollView
    android:scrollbars="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.app.voldoc.DynamicTableActivity">

    <include layout="@layout/test_table_1"
        android:id="@+id/test1_dynamic_table_ll"/>

    <include layout="@layout/test_table_2"
        android:id="@+id/test2_dynamic_table_ll"/>

    <include layout="@layout/test_table_3"
        android:id="@+id/test3_dynamic_table_ll"/>

    <include layout="@layout/test_table_4"
        android:id="@+id/test4_dynamic_table_ll"/>

    <include layout="@layout/test_table_5"
        android:id="@+id/test5_dynamic_table_ll"/>

    <include layout="@layout/test_table_6_a"
        android:visibility="gone"
        android:id="@+id/test6A_dynamic_table_ll"/>



</LinearLayout>
</ScrollView>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <include layout="@layout/test_table_common_footer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"/>

    </RelativeLayout>
</RelativeLayout>