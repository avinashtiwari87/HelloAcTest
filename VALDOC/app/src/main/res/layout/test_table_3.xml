<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:visibility="gone"
        android:id="@+id/test_table_3_header_l_ll"
        style="@style/TextAppearance.AppCompat.header_top"
        android:background="@color/black_overlay">

        <TextView
            style="@style/TextAppearance.AppCompat.header_text"
            android:text="@string/header_title1" />
    </LinearLayout>

<!--    <include layout="@layout/common_test_detail_header"
        android:id="@+id/common_header_test3"/>-->

    <LinearLayout
        android:visibility="gone"
        android:id="@+id/test_table_3_header_2_ll"
        style="@style/TextAppearance.AppCompat.header_top"
        android:layout_marginTop="10dp">
        <TextView
            style="@style/TextAppearance.AppCompat.header_text"
            android:text="@string/header_title_result" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">


                    <TableLayout
                        android:id="@+id/test3_tableLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/measerd_h_value_tv"
                        android:layout_width="match_parent"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        android:visibility="gone"
                        style="@style/TextAppearance.AppCompat.header_text"
                        android:background="@drawable/border1"
                        android:text="Measured Air Flow Quantity(cfm)" />

                    <TableLayout
                        android:id="@+id/test3_tableLayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/room_h_test_tv"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        style="@style/TextAppearance.AppCompat.header_text"
                        android:background="@drawable/border1"
                        android:text="Air Flow Rate (cfm) " />
                    <TableLayout
                        android:id="@+id/test3_tableLayout3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/border"
                        android:padding="0dp" />
</LinearLayout>
                    <TableLayout
                        android:id="@+id/test3_tableLayout4"
                        android:paddingTop="14dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/border"
                        android:padding="0dp" />

                    <TableLayout
                        android:id="@+id/test3_tableLayout5"
                        android:paddingTop="14dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dp"
                        android:background="@drawable/border"
                        android:padding="0dp" />


                </LinearLayout>

            </HorizontalScrollView>

        </LinearLayout>

    </ScrollView>

    <LinearLayout
        android:id="@+id/acph_h_final_calc_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:visibility="gone"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@drawable/border"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/header_text_size"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                android:text="Obtained Test  Results "/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="@dimen/normal_text_size"
                android:textStyle="bold"
                android:background="@drawable/border"
                android:text="Total Airflow Rate"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="@drawable/border"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/normal_text_size"
                android:text="(TFR)"/>
            <TextView
                android:id="@+id/acph_h_tfr_value_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:textSize="@dimen/normal_text_size"
                android:background="@drawable/border"
                android:text="267"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="@dimen/normal_text_size"
                android:textStyle="bold"
                android:background="@drawable/border"
                android:text="No.of Air Changes/hr"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:gravity="center"
                android:textSize="@dimen/normal_text_size"
                android:background="@drawable/border"
                android:text="((TFR/RV)x60))"/>
            <TextView
                android:id="@+id/acph_h_tfrby_av_value_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="@drawable/border"
                android:text="13"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
